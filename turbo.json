{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "*.ts",
        "tsconfig.*"
      ]
    },
    "build:doc": {
      "dependsOn": [
        "^build"
      ]
    },
    "clean": {
      "cache": false
    },
    "coverage": {
      "dependsOn": [
        "@repobuddy/jest#build",
        "build"
      ]
    },
    "depcheck": {
      "dependsOn": [
        "build"
      ]
    },
    "dev": {
      "cache": false
    },
    "lint": {
      "dependsOn": [
        "^build"
      ]
    },
    "size": {
      "dependsOn": [
        "build"
      ]
    },
    "test": {
      "dependsOn": [
        "@repobuddy/jest#build",
        "build"
      ]
    },
    "test:list": {
      "dependsOn": [
        "build"
      ]
    }
  }
}
